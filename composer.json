    "replace": {
        "drupal/drupal": "~8.1"
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/profiles/{$name}": ["type:drupal-profile"]
        },
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
                {
                    "name": "dropzone",
                    "package": "enyo/dropzone"
                }
            ]
        }
    },
    "require-dev": {
    }
